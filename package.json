{
  "name": "@tf2pickup-org/simple-mumble-bot",
  "version": "0.5.0",
  "description": "A simple bot for managing mumble servers",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/tf2pickup-org/simple-mumble-bot",
  "repository": {
    "type": "git",
    "url": "https://github.com/tf2pickup-org/simple-mumble-bot.git"
  },
  "bugs": {
    "url": "https://github.com/tf2pickup-org/simple-mumble-bot/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "test": "jest --watch",
    "test:ci": "NODE_ENV=test jest --ci",
    "test:e2e": "NODE_ENV=test jest --config e2e.jest.config.ts --runInBand",
    "release": "release-it"
  },
  "contributors": [
    "Micha≈Ç Garapich <garrappachc@gmail.com>"
  ],
  "keywords": [
    "mumble",
    "bot"
  ],
  "dependencies": {
    "@tf2pickup-org/mumble-protocol": "0.0.4",
    "rxjs": "7.5.5"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "5.0.0",
    "@tsconfig/node16": "1.0.2",
    "@types/jest": "28.1.1",
    "@types/node": "17.0.31",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "28.1.0",
    "prettier": "2.6.2",
    "release-it": "15.0.0",
    "trace-unhandled": "2.0.1",
    "ts-jest": "28.0.4",
    "ts-node": "10.8.1",
    "tsc-alias": "1.6.9",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.3"
  },
  "packageManager": "yarn@3.2.1"
}
