{
  "name": "@tf2pickup-org/mumble-client",
  "version": "0.12.2",
  "description": "A simple bot for managing mumble servers",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/tf2pickup-org/mumble-client",
  "repository": {
    "type": "git",
    "url": "https://github.com/tf2pickup-org/mumble-client.git"
  },
  "bugs": {
    "url": "https://github.com/tf2pickup-org/mumble-client/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": "./dist/index.js",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write .",
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "test": "vitest",
    "test:e2e": "vitest --config ./e2e/vitest.config.ts",
    "release": "release-it"
  },
  "contributors": [
    "Micha≈Ç Garapich <garrappachc@gmail.com>"
  ],
  "keywords": [
    "mumble",
    "bot"
  ],
  "dependencies": {
    "@protobuf-ts/runtime": "2.9.4",
    "@tf2pickup-org/mumble-protocol": "1.0.8",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@release-it/conventional-changelog": "10.0.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "22.13.5",
    "@types/pem": "1.14.4",
    "eslint": "9.20.1",
    "pem": "1.14.8",
    "prettier": "3.5.1",
    "release-it": "18.1.2",
    "tsc-alias": "1.8.10",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.24.0",
    "vitest": "3.0.6"
  }
}
